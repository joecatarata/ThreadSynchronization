	private Semaphore mutex = new Semaphore(1);
	private Semaphore boarding = new Semaphore(0);
	private Semaphore unboarding = new Semaphore(0);
	private Semaphore doneBoarding = new Semaphore(0);
	private Semaphore doneUnboarding = new Semaphore(0);
	public Car(int C, Semaphore mutex, Semaphore boarding, Semaphore unboarding, Semaphore doneBoarding, Semaphore doneUnboarding) {
		this.C = C;
		this.boarding = boarding;
		this.unboarding = unboarding;
		this.doneBoarding = doneBoarding;
		this.doneUnboarding = doneUnboarding;
	}
	@Override
	public void run() {
		// TODO Auto-generated method stub
		load();
		
	}
	
	public void load() {
		
	}
}
